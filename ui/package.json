    "@vitejs/plugin-vue": "^1.9.3",
    "axios": "^0.22.0",
    "quasar": "^2.1.0",
    "release-it": "^14.11.6",
    "sass": "^1.42.1",
    "typescript": "^4.4.3",
    "vite": "^2.6.5"
  },
  "dependencies": {
    "validator": "^13.6.0"
