{
  "name": "quasar-app-extension-http-authentication",
  "version": "2.0.1",
  "description": "HTTP authentication for Quasar framework",
  "author": "Stefan van Herwijnen",
  "license": "MIT",
  "exports": {
    ".": "./src/index.js",
    "./index": "./src/index.js",
    "./install": "./src/install.js",
    "./uninstall": "./src/uninstall.js",
    "./prompts": "./src/prompts.js",
    "./src/boot/*": {
      "require": "./src/bootd/*",
      "import": "./src/boot/esm/*"
    },
    "./src/*": "./src/*.js"
  },
  "scripts": {
    "build": "rimraf ./src && tsc && tsc -p tsconfig.boot.json && cp -r ./src-ts/templates/ ./src/templates/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stefanvanherwijnen/quasar-app-extension-http-authentication"
  },
  "bugs": "https://github.com/stefanvanherwijnen/quasar-app-extension-http-authentication/issues",
  "homepage": "",
  "dependencies": {
    "quasar-ui-http-authentication": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^16.3.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "quasar": "^2.3.2",
    "quasar-ui-http-authentication": "^2.0.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}